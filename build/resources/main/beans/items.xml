<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

  <bean id="consumableItem" class="com.example.experiments.model.Item.ConsumableItem">
    <property name="name" value="Water"></property>
    <property name="price" value="1.50"></property>
  </bean>

  <bean id="decorItem" class="com.example.experiments.model.Item.DecorItem">
    <property name="name" value="Sofa"></property>
    <property name="category" value="Decora"></property>
    <property name="price" value="899.99"></property>
  </bean>
</beans>
